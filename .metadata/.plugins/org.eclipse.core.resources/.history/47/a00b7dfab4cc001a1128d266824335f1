package com.eomcs.basic.ex04;

public class Exam0430 {
  public static void main(String[] args) {
    char c;
    
    c = 0;
    c= 65535;
    //c = -1;
    //c = 65536;
    
    c = 65;
    c = 0x42;
    c = 0b01000011;
    System.out.println(c);
    
    short i = 65;
    System.out.println(i);
    
    
  }
}
